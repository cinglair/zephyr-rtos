{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"device": "STM32F303VC",
			"cwd": "${workspaceFolder}/",
			"executable": "build/zephyr/zephyr.elf",
			"request": "launch",
			"type": "cortex-debug",
			"runToEntryPoint": "main",
			"toolchainPrefix": "arm-zephyr-eabi",
			"servertype": "openocd",
			"interface": "swd",
			"configFiles": [
					"/usr/share/openocd/scripts/interface/stlink-v2.cfg",
					"/usr/share/openocd/scripts/target/stm32f3x.cfg"
			],
			"preLaunchTask": "West Build",
			"gdbPath": "${userHome}/zephyr-sdk-0.16.3/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
		},
		{
			"name": "Attach",
			"device": "STM32F303VC",
			"cwd": "${workspaceFolder}/",
			"executable": "build/zephyr/zephyr.elf",
			"toolchainPrefix": "arm-zephyr-eabi",
			"request": "attach",
			"type": "cortex-debug",
			"runToEntryPoint": "main",
			"servertype": "openocd",
			"interface": "swd",
			"configFiles": [
					"/usr/share/openocd/scripts/interface/stlink-v2-1.cfg",
					"/usr/share/openocd/scripts/target/stm32f3x.cfg"
			],
			"gdbPath": "${userHome}/zephyr-sdk-0.16.3/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb"
		},
	]
}